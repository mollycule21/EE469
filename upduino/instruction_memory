T_2.3 ;
    %ix/getv 4, v0x55e91ddb47a0_0;
    %load/vec4a v0x55e91ddb52b0, 4;
    %assign/vec4 v0x55e91ddb4f00_0, 0;
T_2.1 ;
    %jmp T_2;
